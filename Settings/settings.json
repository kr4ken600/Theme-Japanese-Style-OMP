{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+\""
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+?"
        },
        {
            "command": "closePane",
            "keys": "ctrl+shift+z"
        },
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+shift+x"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "up"
            }, 
            "keys":"ctrl+shift+up"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "down"
            }, 
            "keys":"ctrl+shift+down"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "left"
            }, 
            "keys":"ctrl+shift+left"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "right"
            }, 
            "keys":"ctrl+shift+right"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
